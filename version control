ssh-keygen -t ed25519 -C "your_email@example.com"
cat ~/.ssh/id_ed25519.pub
eval $(ssh-agent -s)
ssh-add ~/.ssh/id_ed25519
ssh -T git@github.com
origin  git@github.com:rohithh55/Devops---internship---assignment.git (fetch)
origin  git@github.com:rohithh55/Devops---internship---assignment.git (push)
